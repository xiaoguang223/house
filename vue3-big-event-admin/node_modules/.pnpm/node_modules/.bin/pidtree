#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules/pidtree/bin/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules/pidtree/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules/pidtree/bin/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules/pidtree/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/registry.npmmirror.com+pidtree@0.6.0/node_modules:/mnt/c/Users/xiaoqiu/Desktop/软件工程项目/vue3-big-event-admin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pidtree/bin/pidtree.js" "$@"
else
  exec node  "$basedir/../pidtree/bin/pidtree.js" "$@"
fi
